<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jeju_friend.controller.Sign_Controller">
   <children>
      <Label layoutX="592.0" layoutY="182.0" text="관심 지역 선택">
         <font>
            <Font name="LG PC" size="56.0" />
         </font>
      </Label>
      <Label layoutX="135.0" layoutY="182.0" text="남성">
         <font>
            <Font name="LG PC" size="28.0" />
         </font></Label>
      <Label layoutX="321.0" layoutY="182.0" text="여성">
         <font>
            <Font name="LG PC" size="28.0" />
         </font></Label>
      <VBox layoutX="51.0" layoutY="248.0" prefHeight="351.0" prefWidth="393.0">
         <children>
            <Label text="아이디  ">
               <font>
                  <Font name="LG PC" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="idField" onKeyTyped="#idField_Typed" prefHeight="35.0">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin></TextField>
            <Label text="비밀번호">
               <font>
                  <Font name="LG PC" size="30.0" />
               </font>
            </Label>
            <PasswordField fx:id="pwField" maxHeight="-Infinity" minHeight="-Infinity" onKeyTyped="#pwField_Typed" prefHeight="35.0">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </PasswordField>
            <Label text="비밀번호 확인">
               <font>
                  <Font name="LG PC" size="30.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </Label>
            <PasswordField fx:id="pwConfirmField" maxHeight="-Infinity" minHeight="-Infinity" onKeyTyped="#pwConfirmField_Typed" prefHeight="35.0" />
            <Label text="닉네임">
               <font>
                  <Font name="LG PC" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="nameField" maxHeight="-Infinity" prefHeight="35.0">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <Label layoutX="56.0" layoutY="615.0" text="나이">
         <font>
            <Font name="LG PC" size="36.0" />
         </font>
      </Label>
      <Label layoutX="208.0" layoutY="615.0" text="세">
         <font>
            <Font name="LG PC" size="36.0" />
         </font>
      </Label>

      <ToggleButton fx:id="femaleToggleBtn" layoutX="298.0" layoutY="91.0" mnemonicParsing="false" onAction="#femaleToggleBtn_Actioned">
         <graphic>
            <ImageView fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@image/female_icon.png" />
               </image>
            </ImageView>
         </graphic>
   </ToggleButton>
   
      <ToggleButton fx:id="maleToggleBtn" layoutX="112.0" layoutY="91.0" mnemonicParsing="false" onAction="#maleToggleBtn_Actioned">
         <graphic>
            <ImageView fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@image/male_icon.png" />
              </image>
            </ImageView>
          </graphic>
      </ToggleButton>

     
      <ImageView fitHeight="282.0" fitWidth="468.0" layoutX="474.0" layoutY="275.0">
         <image>
            <Image url="@image/map.png" />
         </image>
      </ImageView>
      <Slider fx:id="slider" layoutX="51.0" layoutY="675.0" max="70.0" min="12.0" onMouseDragged="#sliderMouseDragged" prefHeight="48.0" prefWidth="286.0" value="20.0" />
      <Label fx:id="ageLabel" layoutX="159.0" layoutY="706.0" wrapText="true">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Button fx:id="applyBtn" layoutX="801.0" layoutY="633.0" mnemonicParsing="false" onAction="#applyBtn_Actioned" text="확인" wrapText="true">
         <font>
            <Font name="LG PC" size="26.0" />
         </font>
      </Button>
      <Button fx:id="cancelBtn" layoutX="708.0" layoutY="633.0" mnemonicParsing="false" onAction="#cancelBtn_Actioned" text="취소" wrapText="true">
         <font>
            <Font name="LG PC" size="26.0" />
         </font>
      </Button>
   </children>
</Pane>
