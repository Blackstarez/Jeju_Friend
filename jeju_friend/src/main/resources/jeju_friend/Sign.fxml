<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jeju_friend.controller.Sign_Controller">
   <children>
      <Label layoutX="799.0" layoutY="167.0" text="관심 지역 선택">
         <font>
            <Font name="LG PC" size="56.0" />
         </font>
      </Label>
      <Label layoutX="135.0" layoutY="248.0" text="남성">
         <font>
            <Font name="LG PC" size="28.0" />
         </font></Label>
      <Label layoutX="321.0" layoutY="248.0" text="여성">
         <font>
            <Font name="LG PC" size="28.0" />
         </font></Label>
      <VBox layoutX="48.0" layoutY="330.0" prefHeight="351.0" prefWidth="393.0">
         <children>
            <Label text="아이디  ">
               <font>
                  <Font name="LG PC" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="idField" onKeyTyped="#idField_Typed">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin></TextField>
            <Label text="비밀번호">
               <font>
                  <Font name="LG PC" size="30.0" />
               </font>
            </Label>
            <PasswordField fx:id="pwField" maxHeight="-Infinity" minHeight="-Infinity" onKeyTyped="#pwField_Typed">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </PasswordField>
            <Label text="비밀번호 확인">
               <font>
                  <Font name="LG PC" size="30.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </Label>
            <PasswordField fx:id="pwConfirmField" maxHeight="-Infinity" minHeight="-Infinity" onKeyTyped="#pwConfirmField_Typed" />
            <Label text="닉네임">
               <font>
                  <Font name="LG PC" size="30.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="nameField" maxHeight="-Infinity">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <Label layoutX="45.0" layoutY="702.0" text="나이">
         <font>
            <Font name="LG PC" size="36.0" />
         </font>
      </Label>
      <Label layoutX="209.0" layoutY="703.0" text="세">
         <font>
            <Font name="LG PC" size="36.0" />
         </font>
      </Label>

      <ToggleButton fx:id="femaleToggleBtn" layoutX="296.0" layoutY="153.0" mnemonicParsing="false" onAction="#femaleToggleBtn_Actioned">
         <graphic>
            <ImageView fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@image/female_icon.png" />
               </image>
            </ImageView>
         </graphic>
   </ToggleButton>
   
      <ToggleButton fx:id="maleToggleBtn" layoutX="112.0" layoutY="153.0" mnemonicParsing="false" onAction="#maleToggleBtn_Actioned">
         <graphic>
            <ImageView fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@image/male_icon.png" />
              </image>
            </ImageView>
          </graphic>
      </ToggleButton>

     
      <ImageView fitHeight="513.0" fitWidth="748.0" layoutX="595.0" layoutY="210.0">
         <image>
            <Image url="@image/map.png" />
         </image>
      </ImageView>
      <Slider fx:id="slider" layoutX="45.0" layoutY="809.0" max="70.0" min="12.0" onMouseDragged="#sliderMouseDragged" showTickLabels="true" showTickMarks="true" value="20.0" />
      <Label fx:id="ageLabel" layoutX="159.0" layoutY="706.0" text="1">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Pane layoutX="1149.0" layoutY="798.0" prefHeight="99.0" prefWidth="234.0">
         <children>
            <Button fx:id="cancelBtn" layoutX="31.0" layoutY="29.0" mnemonicParsing="false" onAction="#cancelBtn_Actioned" text="취소" wrapText="true">
               <font>
                  <Font name="LG PC" size="26.0" />
               </font>
            </Button>
            <Button fx:id="applyBtn" layoutX="129.0" layoutY="29.0" mnemonicParsing="false" onAction="#applyBtn_Actioned" text="확인" wrapText="true">
               <font>
                  <Font name="LG PC" size="26.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </Pane>
   </children>
</Pane>
